<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users avata_on_post">
  <% @users.each do |user| %>
    <li>
    <%= link_to profile_avatar_select(user),profile_path(user.user_name)%>
      <%= link_to user.user_name,profile_path(user.user_name) %>
      <%= user.email%>
      <% if user_signed_in? && user.admin? == false%>
    	 <%= link_to "delete", user, method: :delete,data: { confirm: "You sure?" } %>
  <% end %>

    </li>
  <% end %>
</ul>
<%= will_paginate %>
